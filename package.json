        "cross-fetch": "^4.0.0",
        "crypto-js": "^4.1.1",
        "express": "4.19.2",
        "helmet": "7.1.0",
        "morgan": "1.10.0",
        "puppeteer": "18.1.0"
    },
    "devDependencies": {
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "husky": "9.0.11",
        "nodemon": "3.1.0",
        "prettier": "3.2.5"
    }
